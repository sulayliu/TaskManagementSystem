@model IEnumerable<string>
@{
    ViewBag.Title = "ShowAllRoles";
}

<h2>ShowAllRoles</h2>


<p>
    @Html.ActionLink("Create Role", "CreateRole")
</p>

<table class="table">
    <tr>
        <th>
            UserName
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item)
            </td>
            <td>
                @Html.ActionLink("Delete", "DeleteRole") |
            </td>
        </tr>
    }

</table>
